! Xdefaults file
urxvt*internalBorder: 5
urxvt*termName:         rxvt
urxvt*scrollBar:        false
urxvt*perl-lib:         /usr/lib/urxvt/perl/
urxvt*perl-ext-common:  default,matcher
urxvt*urlLauncher:      /usr/bin/jumanji
urxvt*matcher.button:   1

urxvt*font: xft:terminus:size=10
urxvt*boldFont: xft:terminus_bold:size=10
urxvt*italicFont: xft:terminus_italic:size=10


!Xft.dpi: 72
!Xft.rgba: rgb
!Xft.antialias: true
Xft.hinting: true
!Xft.autohint: false
!Xft.hintstyle: hintslight
!Xft.lcdfilter: lcddefault

urxvt*background:       rgba:0000/0000/0000/cccc

URxvt.transparent:   true
URxvt.tintColor:     white
URxvt.shading:       20 


urxvt*foreground: #D6C3B6
!urxvt*foreground: #ffdcc4
urxvt*cursorColor: #93a1a1
urxvt*colorIT: #8c644c
urxvt*colorUL: #646a6d
urxvt*colorBD: #746C48

!! black dark/light
urxvt*color0: #332d29
urxvt*color8: #817267

!! red dark/light
urxvt*color1: #8c644c
urxvt*color9: #9f7155

!! green dark/light
urxvt*color2:  #746C48
urxvt*color10: #8F8558

!! yellow dark/light
urxvt*color3: #bfba92
urxvt*color11: #E0DAAC

!! blue dark/light
urxvt*color4: #646a6d
urxvt*color12: #777E82

!! magenta dark/light
!!urxvt*color5: #6d6871
!!urxvt*color13: #756f7b
urxvt*color5: #766782
urxvt*color13: #897796

!! cyan dark/light
!!urxvt*color6: #3b484a
!!urxvt*color14: #444d4e
urxvt*color6: #4B5C5E
urxvt*color14: #556D70

!! white dark/light
urxvt*color7: #504339
urxvt*color15: #9a875f
